        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="365dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
